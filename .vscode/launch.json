{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Flutter (web)",
        "request": "launch",
        "type": "dart",
        "program": "lib/main.dart",
        
        "args": [
          "--web-hostname",
          "0.0.0.0",
          "--web-port",
          "8080"
        ],
        "sourceMaps": true,
        "webDaemonWorkspaceFolder": "${workspaceFolder}/.dart_tool/flutter_web",
        "cwd": "${workspaceFolder}"
      }
    ]
  }
  